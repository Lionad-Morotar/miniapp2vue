{
	"restartable": "rs",
	"verbose": true,
	"watch": [
		"build/miniapp2vue.js",
		"src/swan/**/*"
	],
	"ignore": [
		".git", ".svn", ".tmp", "dist", "node_modules/*/node_modules"
	],
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "js json swan css html wxml",
	"exec": "npm run clear:windows && node --harmony ./build/miniapp2vue.js"
}
